{
  "name": "wendy-panel",
  "version": "0.0.0-beta",
  "private": true,
  "keywords": [
    "monorepo",
    "turbo",
    "vben",
    "vben admin",
    "vben pro",
    "vue",
    "vue admin",
    "vue vben admin",
    "vue vben admin pro",
    "vue3"
  ],
  "homepage": "https://github.com/dstgo/wendy",
  "bugs": "https://github.com/dstgo/wendy/issues",
  "repository": "dstgo/panel.git",
  "license": "MIT",
  "author": {
    "name": "dstgo",
    "email": "dstgo@qq.com",
    "url": "https://github.com/246859"
  },
  "type": "module",
  "scripts": {
    "bootstrap": "pnpm install",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=8192 turbo build",
    "build:analyze": "turbo build:analyze",
    "build:docker": "./build-local-docker-image.sh",
    "build:antd": "pnpm run build --filter=@vben/web-antd",
    "changeset": "pnpm exec changeset",
    "check": "pnpm run check:circular && pnpm run check:dep && pnpm run check:type && pnpm check:cspell",
    "check:circular": "vsh check-circular",
    "check:cspell": "cspell lint **/*.ts **/README.md .changeset/*.md --no-progress",
    "check:dep": "vsh check-dep",
    "check:type": "turbo run typecheck",
    "clean": "vsh clean",
    "commit": "czg",
    "dev": "turbo-run dev",
    "dev:antd": "pnpm -F @vben/web-antd run dev",
    "format": "vsh lint --format",
    "lint": "vsh lint",
    "postinstall": "turbo run stub",
    "preinstall": "npx only-allow pnpm",
    "prepare": "is-ci || husky",
    "preview": "turbo-run preview",
    "publint": "vsh publint",
    "reinstall": "pnpm clean --del-lock && pnpm bootstrap",
    "test:unit": "vitest",
    "update:deps": "pnpm update --latest --recursive",
    "version": "pnpm exec changeset version && pnpm install --no-frozen-lockfile"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.8",
    "@ls-lint/ls-lint": "^2.2.3",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.7.3",
    "@vben/commitlint-config": "workspace:*",
    "@vben/eslint-config": "workspace:*",
    "@vben/prettier-config": "workspace:*",
    "@vben/stylelint-config": "workspace:*",
    "@vben/tailwind-config": "workspace:*",
    "@vben/tsconfig": "workspace:*",
    "@vben/turbo-run": "workspace:*",
    "@vben/vite-config": "workspace:*",
    "@vben/vsh": "workspace:*",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "cspell": "^8.14.4",
    "husky": "^9.1.6",
    "is-ci": "^3.0.1",
    "jsdom": "^24.1.1",
    "lint-staged": "^15.2.10",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.13",
    "turbo": "^2.1.2",
    "typescript": "^5.6.2",
    "unbuild": "^2.0.0",
    "vite": "^5.4.8",
    "vitest": "^2.1.1",
    "vue": "^3.5.9",
    "vue-tsc": "^2.1.6"
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">=9"
  },
  "packageManager": "pnpm@9.7.1",
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "*"
      }
    },
    "overrides": {
      "@ctrl/tinycolor": "4.1.0",
      "clsx": "2.1.1",
      "pinia": "2.2.2",
      "vue": "3.5.9"
    },
    "neverBuiltDependencies": [
      "canvas",
      "node-gyp",
      "playwright"
    ]
  }
}
